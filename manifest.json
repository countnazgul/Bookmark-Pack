{
  "name": "Pack Bookmarks",
  "version": "1.1.3",
  "description": "Simple extension that give and option to pack few bookmarks in single (goo.gl) link",
  "permissions": [
    "http://tuna-optimal.codio.io:8080/*", "bookmarks"
  ],
  "browser_action": {
      "default_title": "Pack Bookmarks",
      "default_icon": "/images/icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}